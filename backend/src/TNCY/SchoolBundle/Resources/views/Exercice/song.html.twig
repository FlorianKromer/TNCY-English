{% extends 'TNCYSchoolBundle::layout.html.twig' %}
{% block title %}{{ "Dashboard"| trans}}  {{ parent() }}  {% endblock %}
{% block container %}


<main class="container">
	<div class="jumbotron">
		<div class="container">
			

			<iframe width="100%" height="166" scrolling="no" frameborder="no" src="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/tracks/{{soundCloundTrackId}}&amp;color=ff5500&amp;auto_play=false&amp;hide_related=false&amp;show_comments=FALSE&amp;show_user=FALSE&amp;show_reposts=false"></iframe>
			<form action="{{ app.request.schemeAndHttpHost ~ app.request.requestUri }}" method="POST">
				
			<p>{{lyrics | raw}}</p>
			
			<button type="submit" class="btn btn-primary">Valider</button>
			</form>

		</div>
	</div>
</main> <!-- /container -->       

{% endblock %}
